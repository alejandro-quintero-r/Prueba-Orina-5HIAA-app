<div class="col-md-8 mx-auto mt-0">
    <div class="card ">
        <div class="card-header text-center bg-secondary">            
            <h1 class="h4 text-white font-weight-bold">Perfil de usuario</h1>
        </div>
        <div class="card-body"> 
                <h5 class="font-weight-bold text-center mt-0 mb-1">Información personal</h5>
                <hr class="mt-0 mb-0">                        
                <div class="form-group row mb-0 mt-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Nombres y apellidos:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.name}} {{user1.lastname}} {{user1.sec_lastname}}</a>    
                    </div>                                         
                </div>  
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Tipo de Identificación:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.identification_type}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Identificación:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.identification}}</a>    
                    </div>                                         
                </div>                

                <h5 class="font-weight-bold text-center mt-0 mb-1">Información general</h5>
                <hr class="mt-0 mb-0 mt-0">
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Fecha de nacimiento:</label>                           
                    </div>                
                    <div>                                                                        
                        <a class="bg-light">{{user1.date_of_bird}}  -  (AAAA/MM/DD)</a> 
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Genero:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.gender}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Tipo de sangre:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.blood_type}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Rh:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.rh}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Estado civil:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.marital_status}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">EPS:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.EPS}}</a>    
                    </div>                                         
                </div> 

                <h5 class="font-weight-bold text-center mt-0 mb-1">Datos de localización</h5>
                <hr class="mt-0 mb-0">
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Teléfono de la casa:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.home_phone}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Teléfono móvil:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.mobile_phone}}</a>    
                    </div>                                         
                </div>
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Teléfono del trabajo:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.work_phone}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Dirección:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.address}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Ciudad:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.city}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Departamento:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.department}}</a>    
                    </div>                                         
                </div>  

                <h5 class="font-weight-bold text-center mt-0 mb-1">Información de sesión</h5>
                <hr class="mt-0 mb-0">
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Correo electrónico:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.email}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Tipo de usuario:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.role}}</a>    
                    </div>                                         
                </div> 
                
                <h5 class="font-weight-bold text-center mt-0 mb-1">Información de contacto</h5>
                <hr class="mt-0 mb-0">
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Nombres y apellidos del contacto:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.contact_name}} {{user1.contact_lastname}} {{user1.contact_sec_lastname}}</a>    
                    </div>                                         
                </div>                
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Relación o parentesco:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.contact_relationship}}</a>    
                    </div>                                         
                </div> 
                <div class="form-group row mb-0">    
                    <div class="col-md-4">
                        <label class="bg-light font-weight-bold">Teléfono:</label>                           
                    </div>                
                    <div >                         
                        <a class="bg-light">{{user1.contact_phone}}</a>    
                    </div>                                         
                </div>
                                                

                

                <div class="form-group row justify-content-center mb-0 mt-4">  
                    <div class="col-md-4">
                        <a href="/users/started "><button class="btn btn-primary btn-block">Ir a inicio</button></a>
                    </div>
                    <div class="col-md-4">
                        <a href="/users/editUserFormAdmin/{{user1._id}}"><button class="btn btn-success btn-block">Editar</button></a>
                    </div> 
                </div>
                
                     
        </div>
    </div>    
</div>
<br>

